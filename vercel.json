{
    "version": 2,
    "builds": [
      {
        "src": "/**/*", 
        "use": "@vercel/node" 
      },
      {
        "src": "src/**/*", 
        "use": "@vercel/static" 
      }
    ],
    "routes": [
      {
        "src": "/dist/(.*)", 
        "dest": "dist/$1"
      },
      {
        "src": "/(.*)", 
        "dest": "public/$1"
      }
    ]
  }
  